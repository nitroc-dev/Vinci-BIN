<header>
    <img src="images/logo.png" alt="logo">
    <nav>
        <ul>
            <li> <a href="/"> Accueil</a> </li>
            <li> <a href="/exoplanets"> Exoplanètes</a> </li>
        </ul>
    </nav>
</header>
<section>
    <h1>Liste des exoplanètes</h1>
    <table>
        <tr>
            <th>Nom Exoplanète</th>
            <th>hClass</th>
            <th>Année de découverte</th>
        </tr>
        {{#each exoplanetsTable}}
        <tr>
            <td>{{this.uniqueName}} </td>
            <td>{{this.hClass}} </td>
            <td>{{this.discoveryYear}} </td>
        </tr>
        {{/each}}
    </table>
</section>
<footer>
    Site sponsorisé par Exo1000.
</footer>